# ************************************************************************************************
# * Remember that the escape character \\ can be used to write a backslash symbol in your code.! *
# ************************************************************************************************

print("              )\n             (\n         ^    )\n        / \ ---\n       /   \| |\n      /  _  \ |\n     /  (_)  \|\n    /         \\ \n   /|   _   _  \ ")
print("    |  | | | |  \ ")
print("    |  |_| |_|  |\ ")
print("    |   __   __ | ")
print("    |  |  | |  ||      # ")
print("    |  | @| |__||     ### ")
print("    |  |  |     |    ##### ")
print(" \/\/\/\/\/\/\/\/\/\/\/\/\/ ")

